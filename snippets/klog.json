{
    "Current date": {
        "prefix": [
            "date",
            "20"
        ],
        "body": "$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE"
    },
    "Record": {
        "prefix": [
            "record"
        ],
        "body": [
            "$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
            "${1:Summary}",
            "    $0"
        ]
    },
    "Timespan": {
        "prefix": [
            "timespan",
            "ts"
        ],
        "body": "${1:$CURRENT_HOUR}:${2:$CURRENT_MINUTE} - ${3:$CURRENT_HOUR}:${4:$CURRENT_MINUTE} $0"
    },
    "Timespan Open-ended": {
        "prefix": [
            "timespan-open-ended",
            "tsoe"
        ],
        "body": "${1:$CURRENT_HOUR}:${2:$CURRENT_MINUTE} - ? $0"
    },
    "Should-total": {
        "prefix": [
            "st",
            "shouldtotal"
        ],
        "body": "(${1:duration}!)"
    }
}

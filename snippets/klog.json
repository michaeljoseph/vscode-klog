{
    "Current date": {
        "prefix": [
            "date",
            "20"
        ],
        "body": "$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE"
    },
    "Record": {
        "prefix": [
            "record"
        ],
        "body": [
            "$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE${1: (${2:duration}!)}",
            "${3:summary}",
            "\t${4:${5:$CURRENT_HOUR}:${6:$CURRENT_MINUTE} - ${7:${8:$CURRENT_HOUR}:${9:$CURRENT_MINUTE}}} $0"
        ]
    },
    "Timespan": {
        "prefix": [
            "timespan",
            "ts"
        ],
        "body": "${1:$CURRENT_HOUR}:${2:$CURRENT_MINUTE} - ${3:${4:$CURRENT_HOUR}:${5:$CURRENT_MINUTE}} $0"
    },
    "Should-total": {
        "prefix": [
            "st",
            "shouldtotal"
        ],
        "body": "(${1:duration}!)"
    }
}

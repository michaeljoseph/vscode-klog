{
    "Current date": {
        "prefix": [
            "date",
            "20"
        ],
        "body": "$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE\n$0"
    },
    "Record": {
        "prefix": "record",
        "body": [
            "$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE ${1:(${2:duration}!)}",
            "${3:summary}",
            "\t${4:${5:00}:${6:00} - ${7:${8:00}:${9:00}}}",
            "$0"
        ]
    }
}

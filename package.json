{
    "name": "vscode-klog",
    "displayName": "Klog support",
    "description": "Highlight and helper for Klog",
    "version": "0.1.0",
    "license": "MIT",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "author": {
        "name": "Vladimirs Nordholm",
        "email": "v@vladde.net",
        "url": "https://vladde.net/"
    },
    "bugs": {
        "url": "https://github.com/vladdeSV/vscode-klog/issues",
        "email": "v@vladde.net"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/vladdeSV/vscode-klog.git"
    },
    "contributes": {
        "languages": [
            {
                "id": "klog",
                "aliases": [
                    "Klog",
                    "klog"
                ],
                "extensions": [
                    ".klg"
                ]
            }
        ],
        "grammars": [
            {
                "language": "klog",
                "scopeName": "source.klog",
                "path": "./syntaxes/klog.json"
            }
        ],
        "snippets": [
            {
                "language": "klog",
                "path": "./snippets/klog.json"
            }
        ]
    }
}
